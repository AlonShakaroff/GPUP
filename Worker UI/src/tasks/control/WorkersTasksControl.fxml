                                       </VBox.margin>
                                    </HBox>
                                    <Label alignment="CENTER" minWidth="60.0" prefWidth="-1.0" style="&#10;" styleClass="headLines" text="Task Progress" textAlignment="CENTER" wrapText="false">
                                       <font>
                                          <Font size="18.0" fx:id="x1111" />
                                       </font>
                                       <textFill>
                                          <Color blue="0.624" green="0.624" red="0.624" fx:id="x2111" />
                                       </textFill>
                                    </Label>
                                    <ProgressBar fx:id="TaskProgressBar" maxWidth="1.7976931348623157E308" progress="0.0" VBox.vgrow="ALWAYS">
                                       <VBox.margin>
                                          <Insets bottom="5.0" left="5.0" right="5.0" />
                                       </VBox.margin>
                                    </ProgressBar>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                     </content>
                  </ScrollPane>
               </items>
            </SplitPane>
         </content>
      </Tab>
