                                          <Button fx:id="PauseTaskButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#PauseTaskButtonClicked" prefWidth="150.0" text="Pause" HBox.hgrow="ALWAYS" />
                                          <Button fx:id="QuitTaskButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#QuitTaskButtonClicked" prefWidth="150.0" text="Quit Task" HBox.hgrow="ALWAYS" />
