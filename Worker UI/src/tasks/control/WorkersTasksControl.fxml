                                          <TableColumn fx:id="TaskStatus" minWidth="30.0" prefWidth="-1.0" text="Status" />
                                          <TableColumn fx:id="AmountOfWorkers" minWidth="13.0" prefWidth="-1.0" text="Amount of workers" />
                                       </columns>
                                       <columnResizePolicy>
                                          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                       </columnResizePolicy>
                                    </TableView>
                                    <HBox alignment="CENTER" spacing="20.0" styleClass="trans">
