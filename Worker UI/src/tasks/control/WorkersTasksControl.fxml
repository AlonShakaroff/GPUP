                                       <textFill>
                                          <Color blue="0.624" green="0.624" red="0.624" fx:id="x211" />
                                       </textFill>
                                    </Label>
                                    <HBox alignment="CENTER" spacing="20.0" styleClass="trans" VBox.vgrow="NEVER">
