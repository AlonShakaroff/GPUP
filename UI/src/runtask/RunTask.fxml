<SplitPane dividerPositions="0.23, 0.56" focusTraversable="true" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="runtask.TaskController">
<items>
      <ScrollPane fitToHeight="true" fitToWidth="true">
         <content>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" styleClass="primaryBackground">
               <children>
                  <Label alignment="CENTER" style="&#10;" styleClass="headLines" text="Choose task" textAlignment="CENTER" wrapText="false" VBox.vgrow="SOMETIMES">
                     <textFill>
                        <Color blue="0.624" green="0.624" red="0.624" fx:id="x21" />
                     </textFill>
                     <font>
                        <Font size="18.0" fx:id="x11" />
                     </font>
                     <VBox.margin>
                        <Insets left="15.0" top="15.0" />
                     </VBox.margin>
                  </Label>
                  <SplitPane dividerPositions="0.47, 0.85" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" VBox.vgrow="SOMETIMES">
                    <items>
                        <VBox spacing="5.0" styleClass="primaryBackground">
                           <children>
                              <Accordion maxHeight="210.0" minHeight="80.0" minWidth="120.0">
                                <panes>
                                  <TitledPane fx:id="simulationTitledPane" animated="false" text="simulation task">
                                       <content>
                                          <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0">
                                             <content>
                                                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" prefHeight="130.0" prefWidth="180.0" spacing="5.0" styleClass="primaryBackground">
                                                   <children>
                                                      <HBox alignment="CENTER_LEFT" spacing="5.0">
                                                         <children>
                                                            <Label text="run time in ms" HBox.hgrow="SOMETIMES" />
                                                            <Spinner fx:id="simulationTimeSpinner" editable="true" maxWidth="130.0" minHeight="30.0" minWidth="50.0" prefHeight="20.0" prefWidth="100.0" HBox.hgrow="SOMETIMES" />
                                                         </children>
                                                      </HBox>
                                                      <CheckBox fx:id="simulationRandomCheckBox" mnemonicParsing="false" prefHeight="30.0" text="random time" VBox.vgrow="SOMETIMES">
                                                         <cursor>
                                                            <Cursor fx:constant="HAND" />
                                                         </cursor>
                                                      </CheckBox>
                                                      <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="15.0" spacing="5.0">
                                                         <children>
                                                            <Label text="success rate (0 - 1) " HBox.hgrow="SOMETIMES" />
                                                            <Spinner fx:id="simulationSuccessRateSpinner" editable="true" maxWidth="100.0" minHeight="30.0" minWidth="50.0" prefHeight="20.0" prefWidth="70.0" HBox.hgrow="SOMETIMES" />
                                                         </children>
                                                      </HBox>
                                                      <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="62.0" spacing="5.0">
                                                         <children>
                                                            <Label text="warning rate (0 - 1)" HBox.hgrow="SOMETIMES" />
                                                            <Spinner fx:id="simulationWarningRateSpinner" editable="true" maxWidth="100.0" minHeight="20.0" minWidth="50.0" prefHeight="30.0" prefWidth="70.0" HBox.hgrow="SOMETIMES" />
                                                         </children>
                                                      </HBox>
                                                   </children>
                                                   <padding>
                                                      <Insets left="5.0" top="5.0" />
                                                   </padding>
                                                </VBox>
                                             </content>
                                          </ScrollPane>
                                       </content>
                                  </TitledPane>
                                  <TitledPane fx:id="compileTaskTitledPane" animated="false" text="compilation task">
                                       <content>
                                          <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="200.0" prefWidth="200.0">
                                             <content>
                                                <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="200.0" spacing="10.0" styleClass="primaryBackground">
                                                   <children>
                                                      <VBox prefWidth="100.0">
                                                         <children>
                                                            <Label text="choose source folder">
                                                               <VBox.margin>
                                                                  <Insets left="2.0" />
                                                               </VBox.margin>
                                                            </Label>
                                                            <HBox alignment="CENTER" maxHeight="27.0" minHeight="27.0">
                                                               <children>
                                                                  <TextField fx:id="compileTaskSourceTextField" editable="false" minWidth="-Infinity" prefHeight="25.0" prefWidth="135.0" HBox.hgrow="ALWAYS" />
                                                                  <Button fx:id="compileTaskSourceSearchButton" mnemonicParsing="false" onAction="#compileTaskSourceSearchButtonClicked" prefHeight="25.0" prefWidth="30.0" styleClass="searchfilebutton" text="choose" HBox.hgrow="NEVER" />
                                                               </children>
                                                               <padding>
                                                                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                                               </padding>
                                                            </HBox>
                                                         </children>
                                                         <VBox.margin>
                                                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                         </VBox.margin>
                                                      </VBox>
                                                      <VBox layoutX="20.0" layoutY="20.0" prefWidth="100.0">
                                                         <children>
                                                            <Label text="choose destination folder">
                                                               <padding>
                                                                  <Insets left="2.0" />
                                                               </padding>
                                                            </Label>
                                                            <HBox alignment="CENTER" maxHeight="27.0" minHeight="27.0">
                                                               <children>
                                                                  <TextField fx:id="compileTaskDestTextField" editable="false" minWidth="-Infinity" prefHeight="25.0" prefWidth="135.0" HBox.hgrow="ALWAYS" />
                                                                  <Button fx:id="compileTaskDestSearchButton" mnemonicParsing="false" onAction="#compileTaskDestSearchButtonClicked" prefHeight="25.0" prefWidth="30.0" styleClass="searchfilebutton" text="choose" HBox.hgrow="NEVER" />
                                                               </children>
                                                               <VBox.margin>
                                                                  <Insets />
                                                               </VBox.margin>
                                                               <padding>
                                                                  <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                                                               </padding>
                                                            </HBox>
                                                         </children>
                                                         <VBox.margin>
                                                            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                         </VBox.margin>
                                                      </VBox>
                                                   </children>
                                                   <padding>
                                                      <Insets left="5.0" right="5.0" top="5.0" />
                                                   </padding>
                                                </VBox>
                                             </content>
                                          </ScrollPane>
                                       </content>
                                  </TitledPane>
                                </panes>
                              </Accordion>
                              <HBox alignment="CENTER_LEFT" spacing="5.0">
                                 <children>
                                    <Label text="parallelism " HBox.hgrow="SOMETIMES">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Label>
                                    <Spinner fx:id="ParallelismSpinner" editable="true" maxWidth="100.0" minHeight="30.0" minWidth="30.0" prefHeight="20.0" prefWidth="40.0" HBox.hgrow="SOMETIMES" />
                                 </children>
                                 <VBox.margin>
                                    <Insets left="10.0" />
                                 </VBox.margin>
                              </HBox>
                              <CheckBox fx:id="incrementalCheckBox" alignment="CENTER" disable="true" mnemonicParsing="false" text="incremental">
                                 <padding>
                                    <Insets left="5.0" right="5.0" />
                                 </padding>
                                 <VBox.margin>
                                    <Insets bottom="10.0" left="5.0" />
                                 </VBox.margin>
                              </CheckBox>
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <Label alignment="CENTER" style="&#10;" styleClass="headLines" text="Selected targets" textAlignment="CENTER" wrapText="false">
                                 <textFill>
                                    <Color blue="0.624" green="0.624" red="0.624" fx:id="x22" />
                                 </textFill>
                                 <font>
                                    <Font size="18.0" fx:id="x12" />
                                 </font>
                                 <VBox.margin>
                                    <Insets left="15.0" />
                                 </VBox.margin>
                              </Label>
                              <ScrollPane fitToHeight="true" fitToWidth="true">
                                 <content>
                                    <ListView fx:id="AddedTargetsListView" />
                                 </content>
                              </ScrollPane>
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </VBox>
                        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" styleClass="primaryBackground">
                           <children>
                              <Button id="playbutton" fx:id="runTaskButton" disable="true" maxHeight="120.0" maxWidth="1.7976931348623157E308" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#runTaskButtonClicked" prefHeight="60.0" prefWidth="60.0" text="run" HBox.hgrow="ALWAYS" />
                              <Button id="pausebutton" fx:id="pauseTaskButton" disable="true" maxHeight="120.0" maxWidth="1.7976931348623157E308" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#runTaskButtonClicked" prefHeight="60.0" prefWidth="60.0" text="pause" HBox.hgrow="ALWAYS" />
                              <Button id="stopbutton" fx:id="stopTaskButton" disable="true" maxHeight="120.0" maxWidth="1.7976931348623157E308" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#stopTaskButtonClicked" prefHeight="60.0" prefWidth="60.0" text="stop" HBox.hgrow="ALWAYS" />
                           </children>
                           <padding>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </padding>
                        </HBox>
                    </items>
                  </SplitPane>
               </children>
               <padding>
                  <Insets bottom="10.0" left="5.0" right="5.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>
  <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="-1.0" prefWidth="-1.0">
    <content>
            <SplitPane dividerPositions="0.5" orientation="VERTICAL">
               <items>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
                     <children>
                        <Label text="Target information" textFill="#9f9f9f">
                           <VBox.margin>
                              <Insets left="15.0" top="15.0" />
                           </VBox.margin>
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                        <ListView fx:id="TargetInfoListView" disable="true" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" />
                     </padding>
                  </VBox>
                  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="175.0" spacing="5.0" styleClass="primaryBackground">
                     <children>
                        <VBox spacing="5.0">
                           <children>
                      <Label alignment="CENTER" style="&#10;" styleClass="headLines" text="Choose targets" textAlignment="CENTER" wrapText="false">
                                 <textFill>
                            <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
                                 </textFill>
                                 <font>
                            <Font size="18.0" fx:id="x1" />
                                 </font>
                              </Label>
                              <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER">
                                 <content>
                                    <ListView fx:id="TargetsListView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
                                 </content>
                              </ScrollPane>
                              <HBox alignment="TOP_CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <Button fx:id="selectAllButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#selectAllButtonClicked" prefWidth="110.0" styleClass="selectionButton" text="select all" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="deSelectAllButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#deSelectAllButtonClicked" prefWidth="110.0" styleClass="selectionButton" text="deselect all" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <HBox alignment="TOP_CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <Button fx:id="allTargetsButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#allTargetsButtonSelected" prefWidth="110.0" styleClass="selectionButton" text="all targets" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="requiredForButton" disable="true" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#requiredForButtonClicked" prefWidth="110.0" styleClass="selectionButton" text="required for" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="dependsOnButton" disable="true" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#dependsOnButtonClicked" prefWidth="110.0" styleClass="selectionButton" text="depends on" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <HBox spacing="5.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <Button fx:id="addButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#addButtonClicked" prefWidth="225.0" styleClass="selectionButton" text="Add" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="removeButton" layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#removeButtonClicked" prefWidth="225.0" styleClass="selectionButton" text="Remove" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="clearButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#clearButtonClicked" prefWidth="225.0" styleClass="selectionButton" text="Clear" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="5.0" right="5.0" />
                     </padding>
                  </VBox>
               </items>
            </SplitPane>
    </content>
  </ScrollPane>
      <VBox prefHeight="671.0" prefWidth="667.0" spacing="5.0" styleClass="primaryBackground">
         <children>
      <Label alignment="CENTER" font="$x1" style="&#10;" styleClass="headLines" text="Details" textAlignment="CENTER" textFill="$x2" wrapText="false">
               <VBox.margin>
                  <Insets left="15.0" top="15.0" />
               </VBox.margin>
            </Label>
            <ScrollPane fitToHeight="true" fitToWidth="true" vbarPolicy="NEVER" VBox.vgrow="SOMETIMES">
               <content>
                  <TableView fx:id="progressTableView" minWidth="300.0" prefWidth="614.0">
                    <columns>
                      <TableColumn prefWidth="75.0" text="in progress" />
                      <TableColumn prefWidth="75.0" text="finished" />
                        <TableColumn prefWidth="75.0" text="waiting" />
                        <TableColumn prefWidth="75.0" text="skipped" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </ScrollPane>
            <TextArea fx:id="runDetailsTextArea" editable="false" prefHeight="100.0" prefWidth="200.0" />
            <ProgressBar id="bar" fx:id="progressBar" maxHeight="-Infinity" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="20.0" progress="0.0" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </ProgressBar>
         </children>
         <padding>
            <Insets bottom="10.0" left="5.0" right="5.0" />
         </padding>
      </VBox>
</items>
</SplitPane>
